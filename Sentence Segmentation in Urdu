      "source": [
        "## Word Tokenization Function"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "wZ2TOEKmwW9u"
      },
      "source": [
        "def word_tokenization(sentence):\n",
        "    temp_word_string=\"\"\n",
        "    final_word_list=[]\n",
        "    index=0\n",
        "    punctuation_mark=[\".\",\"۔\",\"؟\",\"،\",\")\"]\n",
        "    non_closing_pm =[\"‘\"]\n",
        "    \n",
        "    for word in sentence:\n",
        "#         print(word)\n",
        "        index+=1\n",
        "        \n",
        "        if word==\" \" :\n",
        "            if len(temp_word_string)==0 or (sentence[index] in punctuation_mark): \n",
        "                # Final Word of a string is \" \" & no word is present is temp_word_string\n",
        "                pass\n",
        "            else:\n",
        "                final_word_list.append(temp_word_string)\n",
        "                temp_word_string=\"\"\n",
        "            \n",
        "            \n",
        "        elif index==len(sentence):\n",
        "\n",
        "            temp_word_string=temp_word_string[::]+word\n",
        "            final_word_list.append(temp_word_string)\n",
        "            temp_word_string=\"\"\n",
        "        \n",
        "        else:\n",
        "            if  word in punctuation_mark:\n",
        "                temp_word_string=temp_word_string[::]+word\n",
        "                final_word_list.append(temp_word_string)\n",
        "                temp_word_string=\"\"\n",
        "               \n",
        "            else:\n",
        "                temp_word_string=temp_word_string[::]+word\n",
        "    \n",
        "    return final_word_list"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "OyFZPRIGlWNy",
        "scrolled": true
      },
      "source": [
        "# Sentence Segmentation Function \n",
        "# This function will take a sting \n",
        "\n",
        "\n",
        "\n",
        "def urdu_sentence_segmentation(sen1):\n",
        "    \n",
        "    joining_word=[\"کہ\",\"اور\",\"لیکن\",\"لہذا\",\"تو\",\"کہ\",\"یعنی\",\"جس\",\"بلکہ\",\"جبکہ\"]\n",
        "    check_closing_word=[\"ے\",\"ا\",\"ی\",\"ں\",\"و\",\"د\"]\n",
        "    closing_word=[\"تھے\",\"ہیں\",\"ہے\",\"تھا\",\"تھیں\",\"تھی\",\"ہو\",\"ہوں\",\"گا\",\"گی\",\"گے\",\"ہوگا\",\"ہوگی\",\"چاہئے\",\"چکا\"]\n",
        "    closing_symbol=[\".\",\"۔\",\"؟\"]\n",
        "    \n",
        "\n",
        "   \n",
        "    \n",
        "#     word_list = word_tokenization(sen1)  # Spliting word to list\n",
        "    word_list = nltk.word_tokenize(sen1)  # Spliting word to list\n",
        "\n",
        "    \n",
        "    #Initializing Variables\n",
        "    temp_string=\"\"\n",
        "    final_list=[]\n",
        "    index=0\n",
        "    status=\"Y\"\n",
        "    status_h=\"Y\"\n",
        "    sign=\"\"\n",
        "\n",
        "  \n",
        "    \n",
        "    for word in word_list:\n",
        "        \n",
        "        if word==\"\\n\" or (index==len(word_list)-1) :\n",
        "            \n",
        "            temp_string=temp_string+\" \"+word+\"۔\"\n",
        "            final_list.append(temp_string)\n",
        "            temp_string=\"\"\n",
        "            \n",
        "        else:\n",
        "            if word[-1] in closing_symbol: # Checking the [\".\",\"-\",\"؟\"] closing symbol \n",
        "                temp_string=temp_string+\" \"+word\n",
        "                final_list.append(temp_string)\n",
        "                temp_string=\"\"\n",
        "            \n",
        "            elif word[-1] in check_closing_word : # Check the word containing last letter = [\"ے\",\"ا\",\"ی\"]\n",
        "                \n",
        "                if status==\"N\" and index+2<=(len(word_list)-1):\n",
        "                    if status_h==\"Y\":\n",
        "                        status=\"Y\"\n",
        "                elif word_list[index+1] == \"ہوں\" or  word_list[index+1] == \"ہو\" :\n",
        "                    \n",
        "                    if word_list[index+2] in closing_word:\n",
        "\n",
        "                        # inserting complete sentence in the list\n",
        "                        \n",
        "                        temp_string=temp_string+\" \"+word+\" \"+word_list[index+1]+\"۔\"\n",
        "                        final_list.append(temp_string)\n",
        "                        temp_string=\"\"\n",
        "                        status_h=\"N\"\n",
        "                        status=\"N\"\n",
        "                        \n",
        "                    elif word_list[index+1] in closing_word:\n",
        "                        temp_string=temp_string+\" \"+word+\" \"+word_list[index+1]+\"۔\"\n",
        "                        final_list.append(temp_string)\n",
        "                        temp_string=\"\"\n",
        "                        status=\"N\"\n",
        "                        \n",
        "                            \n",
        "                                    \n",
        "                elif word_list[index+1] in closing_word: #sentence completed as next word contain=[\"تھے\",\"ہیں\",\"ہے\",\"تھا\",\"تھیں\",\"تھی\"]\n",
        "                    \n",
        "                    if (word_list[index+2] in joining_word):\n",
        "                        temp_string=temp_string+\" \"+word\n",
        "                    \n",
        "                    elif (word_list[index+2] not in joining_word):\n",
        "                        \n",
        "                        # Check weather next word is a linking word or not\n",
        "                    \n",
        "                        # inserting complete sentence in the list\n",
        "                        temp_string=temp_string+\" \"+word+\" \"+word_list[index+1]+\"۔\"\n",
        "                        final_list.append(temp_string)\n",
        "                        temp_string=\"\"\n",
        "                        status=\"N\" \n",
        "                        \n",
        "                        \n",
        "                    \n",
        "                        \n",
        "                        \n",
        "                else:\n",
        "                    #Check if current word is in closing_word list and next word is not a joining word\n",
        "                    \n",
        "                    if (word in closing_word) and (word_list[index+1] not in closing_word) and (word_list[index+1] not in joining_word) :\n",
        "                        temp_string=temp_string+\" \"+word+\"۔\"\n",
        "                        final_list.append(temp_string)\n",
        "                        temp_string=\"\"\n",
        "#                         print(word,\"  \",word_list[index+1],'  ',word_list[index+2] )\n",
        "\n",
        "                    #Check if current word is in closing_word list and next word is a joining word    \n",
        "                    else:\n",
        "                        \n",
        "                        temp_string=temp_string+\" \"+word\n",
        "\n",
        "\n",
        "            else:\n",
        "                temp_string=temp_string+\" \"+word\n",
        "    \n",
        "        index+=1\n",
        "        \n",
        "        \n",
        "        \n",
        "#     print(\"333333333333333333333333333333333333\")\n",
        "#     for final in final_list:\n",
        "#     print(\"urdu_sentence_segmentation: \",len(final_list),\"\\n\\n\",final_list)\n",
        "    return final_list\n",
        " "
      ],
